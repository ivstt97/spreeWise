{
    "name": "spreewise",
    "private": false,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build": "npm run build:css && remix build",
        "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/app.css",
        "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
        "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
    },
    "dependencies": {
        "@capacitor/android": "^5.5.1",
        "@capacitor/camera": "^5.0.7",
        "@capacitor/cli": "^5.5.1",
        "@capacitor/core": "^5.5.1",
        "@capacitor/ios": "^5.5.1",
        "@prisma/client": "^5.6.0",
        "@remix-run/css-bundle": "^2.3.0",
        "@remix-run/node": "^2.3.0",
        "@remix-run/react": "^2.3.0",
        "async-ocrad": "^0.0.2",
        "bcryptjs": "^2.4.3",
        "isbot": "^3.7.1",
        "minimatch": "^9.0.3",
        "node": "^21.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.12.0",
        "tesseract.js": "^5.0.3"
    },
    "devDependencies": {
        "@flydotio/dockerfile": "^0.4.10",
        "@remix-run/dev": "^2.3.0",
        "@remix-run/eslint-config": "^2.3.0",
        "@remix-run/serve": "^2.3.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/tesseract.js": "^2.0.0",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "eslint": "^8.54.0",
        "postcss": "^8.4.31",
        "prisma": "^5.6.0",
        "remix": "^2.3.0",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
